@model ErrorViewModel

@{
    ViewData["Title"] = "CNJP Existente!!";
}
<h1>@ViewData["Title"]</h1>

<p>CNPJ já existe por favor, cadastre novamente com outro CNPJ!!</p>
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <a asp-action="Create">Voltar</a>
            </div>
        </form>
    </div>
</div>
